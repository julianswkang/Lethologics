{
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Lethologics"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "main.js"
      ],
      "match_about_blank": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This is our version of wordle",
  "icons": {
    "128": "assets/128.png",
    "16": "assets/16.png",
    "48": "assets/48.png"
  },
  "manifest_version": 2,
  "name": "Lethologics",
  "version": "1.0"
}